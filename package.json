{
  "name": "vmsc",
  "displayName": "Visual modeling studio code",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:vmsc"
  ],
  "files": [
    "bin",
    "out",
    "src"
  ],
  "bin": {
    "vmsc-cli": "./bin/cli"
  },
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "vmsc",
        "aliases": [
          "Vmsc",
          "vmsc"
        ],
        "extensions": [
          ".vmsc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vmsc",
        "scopeName": "source.vmsc",
        "path": "./syntaxes/vmsc.tmLanguage.json"
      }
    ],
    "css": [
      {
        "path": "./styles/treeview.css",
        "scope": "resource"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "Vms_code",
          "title": "Vms code",
          "icon": "media/Vms_code.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "VMSC.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "VMSC.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "VMSC.rightClickNode",
        "title": "Right Click"
      },
      {
        "key": "ctrl+R",
        "command": "VMSC.openContextMenu",
        "title": "Open Context Menu"
      },
      {
        "command": "VMSC.openProperties",
        "title": "Open Properties"
      },
      {
        "command": "VMSC.createEcoreModel",
        "title": "Create Ecore Model",
        "icon": {
          "light": "resources/light/create-ecore-model.svg",
          "dark": "resources/dark/create-ecore-model.svg"
        }
      },
      {
        "command": "VMSC.rename",
        "title": "Rename"
      },
      {
        "command": "VMSC.delete",
        "title": "Delete"
      },
      {
        "command": "VMSC.deleteSuperType",
        "title": "Delete"
      },
      {
        "command": "VMSC.addPackage",
        "title": "Add Package"
      },
      {
        "command": "VMSC.addClass",
        "title": "Add Class"
      },
      {
        "command": "VMSC.addDataType",
        "title": "Add Data Type"
      },
      {
        "command": "VMSC.addEnumeration",
        "title": "Add Enumeration"
      },
      {
        "command": "VMSC.addLiteral",
        "title": "Add Literal"
      },
      {
        "command": "VMSC.addAttribute",
        "title": "Add Attribute"
      },
      {
        "command": "VMSC.addReference",
        "title": "Add Reference"
      },
      {
        "command": "VMSC.addOperation",
        "title": "Add Operation"
      },
      {
        "command": "VMSC.addAnnotation",
        "title": "Add Annotation"
      },
      {
        "command": "VMSC.addParameter",
        "title": "Add Parameter"
      },
      {
        "command": "VMSC.addDetailEntry",
        "title": "Add Detail Entry"
      },
      {
        "command": "VMSC.addAssociation",
        "title": "Add Association"
      },
      {
        "command": "VMSC.addGeneralization",
        "title": "Add Generalization"
      },
      {
        "command": "VMSC.addConstraint",
        "title": "Add Constraint"
      }
    ],
    "views": {
      "Vms_code": [
        {
          "id": "exampleView",
          "name": "exampleView"
        }
      ],
      "explorer": [
        {
          "id": "nodeDependencies",
          "name": "Node Dependencies"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "VMSC.rightClickNode",
          "when": "editorTextFocus && nodeTreeItem "
        }
      ],
      "view/title": [
        {
          "command": "VMSC.refreshEntry",
          "when": "view == exampleView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run build && yarn run lint",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "tsc -b tsconfig.json",
    "watch2": "tsc -b tsconfig.json --watch",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "langium:generate": "langium generate",
    "langium:watch": "langium generate --watch"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chevrotain": "^10.4.1",
    "commander": "^8.0.0",
    "jsonc-parser": "^3.2.0",
    "langium": "~1.0.0",
    "vscode-languageclient": "^8.0.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-uri": "^3.0.2"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "langium-cli": "~1.0.0"
  }
}
