{
  "name": "vmsc",
  "displayName": "Visual modeling studio code",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:VMSC.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "Vms_code",
          "title": "Vms code",
          "icon": "media/Vms_code.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "VMSC.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "VMSC.createEcoreModel",
        "title": "Create Ecore Model",
        "icon": {
          "light": "resources/light/create-ecore-model.svg",
          "dark": "resources/dark/create-ecore-model.svg"
        }
      },
      {
        "command": "VMSC.addPackage",
        "title": "Add Package"
      },
      {
        "command": "VMSC.addClass",
        "title": "Add Class"
      },
      {
        "command": "VMSC.addAssociation",
        "title": "Add Association"
      },
      {
        "command": "VMSC.addGeneralization",
        "title": "Add Generalization"
      },
      {
        "command": "VMSC.addEnumeration",
        "title": "Add Enumeration"
      },
      {
        "command": "VMSC.addLiteral",
        "title": "Add Literal"
      },
      {
        "command": "VMSC.addDataType",
        "title": "Add Data Type"
      },
      {
        "command": "VMSC.addAttribute",
        "title": "Add Attribute"
      },
      {
        "command": "VMSC.addOperation",
        "title": "Add Operation"
      }
    ],
    "views": {
      "Vms_code": [
        {
          "id": "exampleView",
          "name": "exampleView"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
