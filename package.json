{
  "name": "vmsc",
  "displayName": "Visual modeling studio code",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:VMSC.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "My Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/my-theme.json"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "Vms_code",
          "title": "Vms code",
          "icon": "media/Vms_code.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "VMSC.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "VMSC.refreshEntry",
        "title": "Refresh"
      },
      {
        "command": "VMSC.rightClickNode",
        "title": "Right Click"
      },
      {
        "key": "ctrl+R",
        "command": "VMSC.openContextMenu",
        "title": "Open Context Menu"
      },
      {
        "command": "VMSC.openProperties",
        "title": "Open Properties"
      },
      {
        "command": "VMSC.createEcoreModel",
        "title": "Create Ecore Model",
        "icon": {
          "light": "resources/light/create-ecore-model.svg",
          "dark": "resources/dark/create-ecore-model.svg"
        }
      },

     
      {
        "command": "VMSC.rename",
        "title": "Rename"
      },
      {
        "command": "VMSC.delete",
        "title": "Delete"
      },
      {
        "command": "VMSC.addPackage",
        "title": "Add Package"
      },
      {
        "command": "VMSC.addClass",
        "title": "Add Class"
      },
      {
        "command": "VMSC.addDataType",
        "title": "Add Data Type"
      },
      {
        "command": "VMSC.addEnumeration",
        "title": "Add Enumeration"
      },

      
      {
        "command": "VMSC.addLiteral",
        "title": "Add Literal"
      },
      {
        "command": "VMSC.addAttribute",
        "title": "Add Attribute"
      },
      {
        "command": "VMSC.addReference",
        "title": "Add Reference"
      },
      {
        "command": "VMSC.addOperation",
        "title": "Add Operation"
      },
      {
        "command": "VMSC.addAnnotation",
        "title": "Add Annotation"
      },

      {
        "command": "VMSC.addParameter",
        "title": "Add Parameter"
      },
      {
        "command": "VMSC.addDetailEntry",
        "title": "Add Detail Entry"
      },

      {
        "command": "VMSC.addAssociation",
        "title": "Add Association"
      },
      {
        "command": "VMSC.addGeneralization",
        "title": "Add Generalization"
      },
      {
        "command": "VMSC.addConstraint",
        "title": "Add Constraint"
      }
    ],
    "views": {
      "Vms_code": [
        {
          "id": "exampleView",
          "name": "exampleView"
        }
      ],
      "explorer": [
        {
          "id": "nodeDependencies",
          "name": "Node Dependencies"
        }
      ]
    },
    "menus": {
      "editor/context": [
          {
            "command": "VMSC.rightClickNode",
            "when": "editorTextFocus && nodeTreeItem "
          }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile && yarn run build && yarn run lint2",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "tsc -b ./server/Vmsc/tsconfig.json",
    "watch2": "tsc -b ./server/Vmsc/tsconfig.json --watch",
    "lint2": "eslint ./server/Vmsc/src --ext ts",
    "langium:generate": "langium generate",
    "langium:watch": "langium generate --watch",
    "test:mochawesome-screenshots": "mocha --reporter mochawesome --reporter-options reportDir=mochawesome-report,reportName=index,inlineAssets=true,overwrite=true,screenshots=true --timeout 5000 ./server/Vmsc/test/suite/*.test.ts",
    "compile2": "tsc -p ./server/Vmsc/",
    "pretest2": "yarn run compile2 && yarn run lint",
    "test2": "node ./server/Vmsc/out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
